{
  "name": "@accolades/powershell-installer",
  "version": "1.0.0",
  "description": "Install powershell on the system",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "tsc && node ./lib/index.js",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test:debian": "jest --config jestconfig.json --group=integration/debian",
    "test:windows": "jest --config jestconfig.json --group=integration/windows",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "powershell"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": "Authfix",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/request": "^2.48.8",
    "jest": "^29.3.1",
    "jest-runner-groups": "^2.2.0",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "os": "^0.1.2",
    "pino": "^8.8.0",
    "request": "^2.88.2"
  }
}
